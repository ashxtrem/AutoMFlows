      "type": "javascriptCode",
      "position": {
        "x": 2100,
        "y": 200
      },
      "data": {
        "type": "javascriptCode",
        "label": "Verify Logout Button",
        "code": "// Verify button 'Log out' is displayed on the new page\nconst logoutButton = context.page.locator('button:has-text(\"Log out\")');\nconst isVisible = await logoutButton.isVisible();\nif (!isVisible) {\n  throw new Error('Log out button is not displayed');\n}\nreturn true;"
      }
    }
