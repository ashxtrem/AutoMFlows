        "jsScript": "// Mobile Viewport Emulation Script\n// This script emulates a mobile device viewport and capabilities\n// Compatible with iPhone 12/13 dimensions (390x844) or iPhone SE (375x667)\n\n(function() {\n  'use strict';\n\n  // Mobile viewport dimensions - iPhone 12/13 Pro dimensions\n  const MOBILE_WIDTH = 390;\n  const MOBILE_HEIGHT = 844;\n\n  // iPhone 12/13 Pro user agent\n  const MOBILE_USER_AGENT = 'Mozilla/5.0 (iPhone; CPU iPhone OS 16_6 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/16.6 Mobile/15E148 Safari/604.1';\n\n  // Override window.innerWidth to return mobile width\n  Object.defineProperty(window, 'innerWidth', {\n    get: () => MOBILE_WIDTH, // Mobile width\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override window.innerHeight to return mobile height\n  Object.defineProperty(window, 'innerHeight', {\n    get: () => MOBILE_HEIGHT, // Mobile height\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override window.outerWidth\n  Object.defineProperty(window, 'outerWidth', {\n    get: () => MOBILE_WIDTH,\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override window.outerHeight\n  Object.defineProperty(window, 'outerHeight', {\n    get: () => MOBILE_HEIGHT,\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override screen.width\n  Object.defineProperty(screen, 'width', {\n    get: () => MOBILE_WIDTH,\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override screen.height\n  Object.defineProperty(screen, 'height', {\n    get: () => MOBILE_HEIGHT,\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override screen.availWidth\n  Object.defineProperty(screen, 'availWidth', {\n    get: () => MOBILE_WIDTH,\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override screen.availHeight\n  Object.defineProperty(screen, 'availHeight', {\n    get: () => MOBILE_HEIGHT - 44, // Account for status bar\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override navigator.userAgent with mobile user agent\n  Object.defineProperty(navigator, 'userAgent', {\n    get: () => MOBILE_USER_AGENT,\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override navigator.platform\n  Object.defineProperty(navigator, 'platform', {\n    get: () => 'iPhone',\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override navigator.maxTouchPoints to indicate touch support\n  Object.defineProperty(navigator, 'maxTouchPoints', {\n    get: () => 5,\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override navigator.hardwareConcurrency (mobile devices typically have fewer cores)\n  Object.defineProperty(navigator, 'hardwareConcurrency', {\n    get: () => 6,\n    configurable: true,\n    enumerable: true\n  });\n\n  // Override navigator.deviceMemory (if available)\n  if ('deviceMemory' in navigator) {\n    Object.defineProperty(navigator, 'deviceMemory', {\n      get: () => 4, // 4GB typical for modern iPhone\n      configurable: true,\n      enumerable: true\n    });\n  }\n\n  // Override connection type for mobile\n  if ('connection' in navigator) {\n    const connection = navigator.connection || navigator.mozConnection || navigator.webkitConnection;\n    if (connection) {\n      Object.defineProperty(connection, 'effectiveType', {\n        get: () => '4g',\n        configurable: true,\n        enumerable: true\n      });\n    }\n  }\n\n  // Add touch event support\n  if (!('ontouchstart' in window)) {\n    Object.defineProperty(window, 'ontouchstart', {\n      get: () => null,\n      set: () => {},\n      configurable: true,\n      enumerable: true\n    });\n  }\n\n  // Override matchMedia to return mobile viewport\n  const originalMatchMedia = window.matchMedia;\n  window.matchMedia = function(query) {\n    if (query.includes('max-width')) {\n      // Return a mock MediaQueryList that matches mobile viewport\n      const match = query.match(/max-width:\\s*(\\d+)px/);\n      if (match &